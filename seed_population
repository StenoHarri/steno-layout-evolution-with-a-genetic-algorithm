def generate_chords(left_bank_length = 7, right_bank_length = 8, max_chords = 50, left_chords = [], right_chords = []):

    for left_chord_number in range(max_chords - len(left_chords)):
        return

    for right_chord_number in range(max_chords - len(right_chords)):
        return


def create_initial_population(left_bank_length, right_bank_length, max_chords = 50, population_size = 3, left_chords = [], right_chords = []):

    population = []

    for individual in population_size:
        population+= generate_chords()

    return

